{
  "typescript.preferences.renameMatchingJsxTags": true,
  "typescript.preferences.jsxAttributeCompletionStyle": "auto",
  "markdownlint.onSave": true,
  "javascript.autoClosingTags": true,
  "typescript.autoClosingTags": true,
  "rust-analyzer.checkOnSave": false,
  "suggest.autoTrigger": "always",
  "html.autoClosingTags": true,
  "java.server.launchMode": "Standard",
  "suggest.insertMode": "replace",
  "colors.enable": true,
  "coc.preferences.bracketEnterImprove": true,
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "java.showBuildStatusOnStart.enabled": "notification",
  "Lua.window.statusBar": true,
  "java.format.enabled": true,
  "tailwindCSS.hovers": true,
  "Lua.workspace.checkThirdParty": false,
  "emmet.includeLanguages": {
    "xml": "xml",
    "vue-html": "html",
    "javascript": "javascriptreact",
    "tailwindcss": ["javascriptreact", "typescriptreact"]
  },
  "coc.preferences.formatOnSave": true,
  "dialog.confirmKey": "<cr>",
  "notification.border": true,
  "workspace.workspaceFolderFallbackCwd": true,
  "suggest.snippetsSupport": true,
  "javascript.preferences.renameMatchingJsxTags": true,
  "dialog.rounded": true,
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/18.1.3/clangd_18.1.3/bin/clangd",
  "tsserver.implicitProjectConfig.checkJs": true,
  "list.smartCase": false,
  "tailwindCSS.suggestions": true,
  "snippets.ultisnips.pythonPrompt": false,
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.work", "go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  },
  "svelte.enable-ts-plugin": true,
  "svelte.plugin.html.enable": true,
  "svelte.plugin.css.completions.enable": true,
  "svelte.plugin.html.completions.enable": true,
  "prettier.configPath": "~/svelte-app/.prettierrc"
}
